<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.centrestfoodpantry.pantryApp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/pantrylogo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="org.centrestfoodpantry.pantryApp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="org.centrestfoodpantry.pantryApp.DisplayMessageActivity" />
        <activity android:name="org.centrestfoodpantry.pantryApp.HelloGridView" />
        <activity android:name="org.centrestfoodpantry.pantryApp.ListActivity"></activity>
    </application>

</manifest>